<% if @updated %>
  var ele = $('#profile-cover');
  if(ele.is('img')){
    ele.attr('src', '<%= current_user.cover.url(:medium) %>');
  }else{
    ele.css('background-image', "url(<%= current_user.cover.url(:medium) %>)")  
  }
<% else %>
  showAlert('error', 'Cover has failed to upload due to an error.');
<% end %>
